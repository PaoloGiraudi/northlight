{
  "name": "@mediatool/tokens",
  "version": "1.1.1",
  "license": "UNLICENSED",
  "exports": {
    "import": "./dist/es/tokens.mjs",
    "require": "./dist/cjs/tokens.js"
  },
  "types": "./dist/ts/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "transform": "yarn token-transformer lib/tokens.json dist/sets/global.json global && yarn token-transformer lib/tokens.json dist/sets/theme.json global,theme global",
